head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width' ,='' initial-scale='1.0')
  title Login into WeNaari
  link(rel='preconnect' href='https://fonts.googleapis.com')
  link(rel='preconnect' href='https://fonts.gstatic.com' crossorigin='')
  link(href='https://fonts.googleapis.com/css2?family=Dancing+Script:wght@700&family=Oleo+Script+Swash+Caps&display=swap' rel='stylesheet')
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css')
  link(rel="stylesheet" href = "/style/login.css")
section
  .imgBx
    img(src='data:image/jpeg;base64,')
    .imgText
      .welcome
        | Welcome back at WeNari!
  .contentBx
    .center
      h1 LOGIN INTO YOUR ACCOUNT
      div.err 
        if(error)
          p Invalid E-mail or Password
      form(method='post' action='/login')
        input(type="hidden" name="_csrf" value=`${csrfToken}`)

        #user.TxtField
          label Email Address
          input(type='email' value=`${error ? email : ''}` required='' name='email')
          span
        .TxtField
          label Password
          .d-flex
            input#password(type='password' value=`${error ? password : ''}` required='' name='password')
            input#eye(type = 'checkbox' onclick = 'show()')
          span
        .login
          a(href='/forgetPass')
            | Forgot Password? 
            br
            br
        #register.TxtField
          button(type='submit' class='btn') Login
        #newacc.login
          p 
            | New at WeNaari? 
            a(href='/signup') Create an account

block scripts
  script(src = '/js/script.js')